{
  "queryPath": "leadEngineers",
  "mutation": "LEADENGINEER",
  "query": "GET_LEAD_ENGINEER",
  "chapters": [
    {
      "pages": [
        {
          "pageTitle": "Steel Preparation",
          "queryPath": "leadEngineers.0",
          "fields": [
            {
              "fieldName": "steel",
              "label": "Steel",
              "subtext": "Subtext",
              "type": "select",
              "options": ["", "Carbon", "Duplex"]
            },
            {
              "fieldName": "blastMedia",
              "label": "Blast Media",
              "unit": "Unit",
              "subtext": "Subtext",
              "type": "select",
              "options": ["", "Garnet", "Grit", "Aluminum Oxide"]
            },
            {
              "fieldName": "relativeHumidity",
              "label": "Relative Humidity",
              "default": 85,
              "type": "Number"
            },
            {
              "fieldName": "airTemperature",
              "label": "Air Temperature",
              "default": 15,
              "type": "Number"
            },
            {
              "fieldName": "steelTemperature",
              "label": "Steel Temperature",
              "default": 15,
              "type": "Number"
            },
            {
              "fieldName": "temperatureOverDewPoint",
              "label": "Temperature over Dew Point",
              "default": 15,
              "type": "Number"
            },
            {
              "fieldName": "blastMediaConductivity",
              "label": "Blast Media Conductivity",
              "type": "Number"
            },
            {
              "fieldName": "surfaceProfileMin",
              "label": "Surface Profile (Roughnes)",
              "placeholder": "Min",
              "default": 50,
              "type": "Number"
            },
            {
              "fieldName": "surfaceProfileMax",
              "label": "Surface Profile (Roughnes)",
              "placeholder": "Max",
              "default": 100,
              "type": "Number"
            },
            {
              "fieldName": "solubleSaltLevel",
              "label": "Soluble Salt Level",
              "default": 20,
              "type": "Number"
            },
            {
              "fieldName": "inspectionOfSteelSurface",
              "label": "Inspection of Steel Surface",
              "type": "checkbox"
            },
            {
              "fieldName": "compressedAirCheck",
              "label": "Compressed Air Check",
              "placeholder": "Min",
              "type": "checkbox"
            },
            {
              "fieldName": "surfaceCleanlines",
              "label": "Surface Cleanlines",
              "placeholder": "Max",
              "type": "checkbox"
            },
            {
              "fieldName": "uvTest",
              "label": "UV Test",
              "type": "checkbox"
            },
            {
              "fieldName": "primer1",
              "label": "Primer 1",
              "type": "select",
              "options": [
                "Chemosil 211",
                "Chemosil 231",
                "Chemosil 360",
                "Chemosil 411"
              ],
              "required": true
            },
            {
              "fieldName": "primer2",
              "label": "Primer 2",
              "type": "select",
              "options": [
                "Chemosil 211",
                "Chemosil 231",
                "Chemosil 360",
                "Chemosil 411"
              ]
            },
            {
              "fieldName": "rubberCementsBeforeCoating",
              "label": "Rubber Cement Before Coating",

              "type": "select",
              "options": ["None", "75051", "Soap"]
            }
          ]
        }
      ]
    },
    {
      "pages": [
        {
          "pageTitle": "Coating and Vulcanzation",
          "query": "GET_LEAD_ENGINEER",
          "queryPath": "leadEngineers.0",
          "mutation": "LEADENGINEER",
          "fields": [
            {
              "fieldName": "targetDescriptionValue",
              "label": "TDV (Target Description Value)",
              "placeholder": "Min",
              "type": "select",
              "options": ["", "OD", "ID"]
            },
            {
              "fieldName": "orderedTotalRubberThicknes",
              "label": "Ordered Total Rubber Thicknes",
              "type": "number",
              "required": true
            },
            {
              "fieldName": "toleranceMin",
              "label": "Tolerance Min",
              "prepend": "Min",
              "type": "number"
            },
            {
              "fieldName": "toleranceMinPercent",
              "label": "Tolerance Min in Percent",
              "math": "mathToleranceMinPercent",
              "type": "Number",
              "min": 2
            },
            {
              "fieldName": "toleranceMax",
              "label": "Tolerance Max",
              "prepend": "Max",
              "type": "number"
            },
            {
              "fieldName": "toleranceMaxPercent",
              "label": "Tolerance Max in Percent",
              "math": "mathToleranceMaxPercent",
              "type": "Number",
              "max": 6
            },
            {
              "fieldName": "measurementPoint",
              "label": "Measurement Points",
              "maxInput": 22,
              "max": 22
            }
          ]
        },
        {
          "pageTitle": "Measurement Point Actual",
          "queryPath": "leadEngineers.0.measurementPointActualTvds",
          "repeat": true,
          "notAddButton": true,
          "repeatGroupWithQuery": "leadEngineers.0.data.measurementPoint",
          "fields": [
            {
              "fieldName": "measurementPointActual",
              "queryVariableLabel": "leadEngineers.0.data.targetDescriptionValue",
              "label": "Measurement Point Actual Steel {}",
              "type": "number"
            }
          ]
        },
        {
          "pageTitle": "Rubber Cement",
          "queryPath": "leadEngineers.0.rubberCements",
          "repeat": true,
          "delete": true,
          "addButton": "Add Rubber Cement",
          "fields": [
            {
              "fieldName": "rubberCement",
              "label": "Rubber Cement",
              "notLabel": true,
              "type": "number"
            }
          ]
        },
        {
          "queryPath": "leadEngineers.0.vulcanizationSteps",
          "repeat": true,
          "repeatStartWithOneGroup": true,
          "delete": true,
          "addButton": "Add Step",
          "pageTitle": "Vulcanization Steps",
          "indexVariableFieldGroupRepeatTitle": true,
          "fieldGroupRepeatTitle": "Step {}",
          "fields": [
            {
              "fieldName": "vulcanizationOption",
              "label": "Vulcanization Option",
              "type": "select",
              "options": [
                "",
                "Air Autoclave",
                "Heating Blankets",
                "Heating Cables",
                "Hot Air",
                "Steam Autoclave"
              ]
            },
            {
              "fieldName": "programNumber",
              "label": "Program Number",
              "type": "Number"
            },
            {
              "fieldName": "numberOfLayers",
              "label": "Number of Layers",
              "type": "Number"
            },
            {
              "page": true,
              "pageTitle": "Coating Layer",
              "queryPath": "coatingLayers",
              "repeat": true,
              "notAddButton": true,
              "repeatGroupWithQuery": [
                "leadEngineers.0.vulcanizationSteps",
                "data.numberOfLayers"
              ],
              "fields": [
                {
                  "fieldName": "layer",
                  "label": "Layer",
                  "setValueByIndex": true,
                  "type": "Number",
                  "textCenter": true
                },
                { "line": true },
                {
                  "fieldName": "compoundNumber",
                  "label": "Compound Number",
                  "type": "select",
                  "options": ["Compound Number", 73165]
                },
                {
                  "fieldName": "actualThicknes",
                  "label": "Actual Thicknes",
                  "type": "Number",
                  "min": 2,
                  "max": 4,
                  "unit": "mm",
                  "decimal": 2,
                  "required": true
                },
                {
                  "fieldName": "shrink",
                  "label": "Shrink",
                  "type": "Number",
                  "max": 15,
                  "min": 2,
                  "required": true
                },
                {
                  "fieldName": "proposedThicknes",
                  "label": "Proposed Thicknes",
                  "math": "mathProposedThicknes",
                  "min": 2,
                  "max": 4,
                  "unit": "mm",
                  "decimal": 1,
                  "type": "Number"
                },
                {
                  "fieldName": "layersUnique",
                  "label": "Layers Unique",
                  "type": "checkbox"
                },
                {
                  "page": true,
                  "queryPath": "cumulativeThicknes",
                  "repeat": true,
                  "notAddButton": true,
                  "repeatGroupWithQuery": "leadEngineers.0.data.measurementPoint",
                  "fields": [
                    {
                      "fieldName": "cumulativeThicknes",
                      "label": "Cumulative Thicknes {}",
                      "math": "mathCumulativeThicknes",
                      "indexVariableLabel": true,
                      "decimal": 1,
                      "type": "Number"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
