            {
              "fieldName": "equipmentIDUvTest",
              "showFieldSpecPath": "leadEngineers.0.data.uvTest",
              "placeholder": "Equipment ID"
            },
            {
              "showFieldSpecPath": "leadEngineers.0.data.uvTest",
              "fieldName": "nextCalibrationDateUvTest",
              "label": "Next Calibration Date",
