            {
              "fieldName": "nextCalibrationDateMediaBlasting",
              "label": "Next Calibration Date",
              "type": "date"
            },
            {
              "fieldName": "mediaBlastingStarted",
              "label": "Media Blasting Started",
              "type": "datetime-local"
            }
          ]
        }
      ]
    },
