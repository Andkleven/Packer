              "type": "number"
            },
            {
              "showFieldSpecPath": "leadEngineers.0.data.surfaceCleanliness",
              "fieldName": "equipmentIDSurfaceProfileRoughness",
              "placeholder": "Equipment ID"
            },
            {
              "showFieldSpecPath": "leadEngineers.0.data.surfaceCleanliness",
