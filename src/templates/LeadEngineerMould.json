{
  "queryPath": "leadEngineers",
  "mutation": "LEADENGINEER",
  "query": "GET_LEAD_ENGINEER",
  "chapters": [
    {
      "pages": [
        {
          "pageTitle": "Steel Preparation",
          "queryPath": "leadEngineers",
          "fields": [
            {
              "fieldName": "relativeHumidity",
              "label": "Relative Humidity",
              "default": 85,
              "type": "Number"
            },
            {
              "fieldName": "airTemperature",
              "label": "Air Temperature",
              "default": 15,
              "unit": "°C",
              "type": "Number"
            },
            {
              "fieldName": "steelTemperature",
              "label": "Steel Temperature",
              "default": 15,
              "unit": "°C",
              "type": "Number"
            },
            {
              "fieldName": "temperatureOverDewPoint",
              "label": "Temperature over Dew Point",
              "default": 3,
              "unit": "°C",
              "type": "Number"
            },
            {
              "fieldName": "rubberCementsBeforeCoating",
              "label": "Release Agent",
              "type": "select",
              "options": ["75051", "Soap"]
            }
          ]
        }
      ]
    },
    {
      "pages": [
        {
          "pageTitle": "Coating and Vulcanzation",
          "query": "GET_LEAD_ENGINEER",
          "queryPath": "leadEngineers",
          "mutation": "LEADENGINEER",
          "fields": [
            {
              "fieldName": "orderedTotalRubberThicknes",
              "label": "Ordered Total Rubber Thicknes",
              "type": "number",
              "required": true
            },
            {
              "fieldName": "toleranceMin",
              "label": "Tolerance Min",
              "prepend": "Min",
              "type": "number"
            },
            {
              "fieldName": "toleranceMinPercent",
              "label": "Tolerance Min in Percent",
              "unit": "%",
              "math": "mathToleranceMinPercent",
              "type": "Number",
              "min": 2
            },
            {
              "fieldName": "toleranceMax",
              "label": "Tolerance Max",
              "prepend": "Max",
              "type": "number"
            },
            {
              "fieldName": "toleranceMaxPercent",
              "label": "Tolerance Max in Percent",
              "unit": "%",
              "math": "mathToleranceMaxPercent",
              "type": "Number",
              "max": 6
            },
            {
              "fieldName": "measurementPoint",
              "label": "Measurement Points",
              "maxInput": 22,
              "max": 22
            }
          ]
        },
        {
          "queryPath": "leadEngineers.0.vulcanizationSteps",
          "repeat": true,
          "repeatStartWithOneGroup": true,
          "delete": true,
          "addButton": "Add Step",
          "pageTitle": "Vulcanization",
          "indexVariableFieldGroupRepeatTitle": true,
          "fieldGroupRepeatTitle": "Step {}",
          "fields": [
            {
              "fieldName": "vulcanizationOption",
              "label": "Vulcanization Option",
              "type": "select",
              "options": [
                "Air Autoclave",
                "Heating Blankets",
                "Heating Cables",
                "Hot Air",
                "Steam Autoclave"
              ]
            },
            {
              "fieldName": "programNumber",
              "label": "Program Number",
              "type": "Number"
            },
            {
              "fieldName": "numberOfLayers",
              "label": "Number of Layers",
              "type": "Number"
            },
            {
              "page": true,
              "pageTitle": "Coating",
              "queryPath": "coatingLayers",
              "repeat": true,
              "notAddButton": true,
              "repeatGroupWithQuery": [
                "leadEngineers.0.vulcanizationSteps",
                "data.numberOfLayers"
              ],
              "fields": [
                {
                  "fieldName": "layer",
                  "label": "Layer",
                  "setValueByIndex": true,
                  "type": "Number",
                  "textCenter": true
                },
                { "line": true },
                {
                  "fieldName": "compoundNumber",
                  "label": "Compound Number",
                  "type": "select",
                  "options": [73165]
                },
                {
                  "fieldName": "actualThicknes",
                  "label": "Actual Thicknes",
                  "type": "Number",
                  "min": 2,
                  "max": 4,
                  "unit": "mm",
                  "decimal": 2,
                  "required": true
                },
                {
                  "fieldName": "shrink",
                  "label": "Shrink",
                  "type": "Number",
                  "max": 15,
                  "min": 2,
                  "required": true
                },
                {
                  "fieldName": "proposedThicknes",
                  "label": "Proposed Thicknes",
                  "math": "mathProposedThicknes",
                  "min": 2,
                  "max": 4,
                  "unit": "mm",
                  "decimal": 1,
                  "type": "Number"
                },
                {
                  "fieldName": "layersUnique",
                  "label": "Layers Unique",
                  "type": "checkbox"
                },
                {
                  "page": true,
                  "queryPath": "cumulativeThicknes",
                  "repeat": true,
                  "notAddButton": true,
                  "repeatGroupWithQuery": "leadEngineers.0.data.measurementPoint",
                  "fields": [
                    {
                      "fieldName": "cumulativeThicknes",
                      "label": "Cumulative Thicknes {}",
                      "math": "mathCumulativeThicknes",
                      "indexVariableLabel": true,
                      "decimal": 1,
                      "type": "Number"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "pages": [
            {
              "pageTitle": "Final Inspection",
              "queryPath": "leadEngineers",
              "fields": [
                {
                  "fieldName": "hardnessOfOuterLayer",
                  "label": "Hardness of Outer Layer",
                  "unit": "Shore A"
                },
                {
                  "fieldName": "identificationMarking",
                  "label": "Identification Marking"
                },
                {
                  "fieldName": "peelTest",
                  "label": "Peel Test",
                  "type": "N/25mm"
                },
                {
                  "fieldName": "totalCoatingThickness",
                  "label": "Total Coating Thickness",
                  "type": "checkbox"
                },
                {
                  "fieldName": "visualInspection",
                  "label": "Visual Inspection",
                  "type": "checkbox"
                },
                {
                  "fieldName": "finalDimensionsCheck",
                  "label": "Final Dimensions Check",
                  "type": "checkbox"
                },
                {
                  "fieldName": "sparkTest",
                  "label": "Spark Test",
                  "type": "checkbox"
                },
                {
                  "fieldName": "hammerTest",
                  "label": "Hammer Test",
                  "type": "checkbox"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
